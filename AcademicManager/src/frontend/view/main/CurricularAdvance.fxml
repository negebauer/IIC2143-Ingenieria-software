<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.view.main.CurricularAdvanceController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="labelCurricularAdvanceWelcomeMessage" layoutX="14.0" layoutY="80.0" prefHeight="42.0" prefWidth="299.0" text="Este es tu avance curricular, selecciona otra malla curricular para ver tu avance" wrapText="true" />
            <ChoiceBox fx:id="chBxStudyProgram" layoutX="52.0" layoutY="126.0" prefHeight="25.0" prefWidth="172.0" style="-fx-background-radius: 0;" />
            <ListView fx:id="listAdvance" layoutX="222.0" layoutY="100.0" prefHeight="200.0" prefWidth="162.0" visible="false" />
            <ListView fx:id="listUnfinished" layoutX="362.0" layoutY="100.0" prefHeight="200.0" prefWidth="162.0" visible="false" />
            <Button fx:id="btnShowAdvance" layoutX="435.0" layoutY="326.0" mnemonicParsing="false" onAction="#btnShowAdvance_Pressed" style="-fx-background-radius: 0;" text="Guardar Semestre" />
         </children>
      </Pane>
      <Button fx:id="btnLogout" layoutX="531.0" layoutY="369.0" mnemonicParsing="false" onAction="#btnLogout_Pressed" style="-fx-background-radius: 0;" text="Logout" />
      <Button fx:id="btnBack" layoutX="24.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnBack_Pressed" style="-fx-background-radius: 0;" text="Back" />
      <Button fx:id="btnReload" layoutX="518.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnReload_Pressed" style="-fx-background-radius: 0;" text="R" />
      <Button fx:id="btnLanguage" layoutX="561.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnLanguage_Pressed" style="-fx-background-radius: 0;" text="ES" />
   </children>
</AnchorPane>
