<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.admin.AEvaluationManagerMainViewController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="labelEvaluationsEditor" layoutX="34.0" layoutY="80.0" prefHeight="42.0" prefWidth="272.0" text="Editor de Evaluaciones, selecciona una evaluacion para editar o crea una nueva" wrapText="true" />
            <ChoiceBox fx:id="chBxEvaluations" layoutX="71.0" layoutY="144.0" prefWidth="150.0" style="-fx-background-radius: 0;" />
            <Button fx:id="btnEditEvaluation" layoutX="71.0" layoutY="187.0" mnemonicParsing="false" onAction="#btnEditEvaluation_Pressed" style="-fx-background-radius: 0;" text="Editar evaluacion" />
            <Button fx:id="btnCreateEvaluation" layoutX="71.0" layoutY="307.0" mnemonicParsing="false" onAction="#btnCreateEvaluation_Pressed" style="-fx-background-radius: 0;" text="Crear nueva evaluacion" />
            <Button fx:id="btnRemoveEvaluation" layoutX="71.0" layoutY="224.0" mnemonicParsing="false" onAction="#btnRemoveEvaluation_Pressed" style="-fx-background-radius: 0;" text="Eliminar evaluacion" />
         </children>
      </Pane>
      <Button fx:id="btnLogout" layoutX="531.0" layoutY="369.0" mnemonicParsing="false" onAction="#btnLogout_Pressed" style="-fx-background-radius: 0;" text="Logout" />
      <Button fx:id="btnBack" layoutX="24.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnBack_Pressed" style="-fx-background-radius: 0;" text="Back" />
      <Button fx:id="btnReload" layoutX="518.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnReload_Pressed" style="-fx-background-radius: 0;" text="R" />
      <Button fx:id="btnLanguage" layoutX="561.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnLanguage_Pressed" style="-fx-background-radius: 0;" text="ES" />
   </children>
</AnchorPane>
