<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.admin.ACourseManagerMainViewController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="labelCourseEditorWelcomeMessage" layoutX="120.0" layoutY="14.0" prefHeight="40.0" prefWidth="360.0" text="Bienvenido al editor de cursos, para comenzar selecciona un curso de la lista para editarlo, o bien, selecciona en crear nuevo curso" wrapText="true" />
            <Button fx:id="btnCreateNewCourse" layoutX="44.0" layoutY="329.0" mnemonicParsing="false" onAction="#btnCreateNewCourse_Pressed" style="-fx-background-radius: 0;" text="Crear Curso Nuevo" />
            <Label fx:id="labelSelectCoure" layoutX="273.0" layoutY="155.0" text="Elige ramos" />
            <ChoiceBox fx:id="chBxSelectedCourse" layoutX="208.0" layoutY="178.0" prefHeight="27.0" prefWidth="193.0" />
            <Label fx:id="labelSearchCourse" layoutX="269.0" layoutY="67.0" text="Buscar ramo" />
            <TextField fx:id="txBxCourseToSearch" layoutX="211.0" layoutY="85.0" prefHeight="27.0" prefWidth="193.0" />
            <Button fx:id="btnSearchCourse" layoutX="254.0" layoutY="115.0" mnemonicParsing="false" onAction="#btnSearchCourse_Pressed" prefHeight="27.0" prefWidth="102.0" text="Search" />
            <Button fx:id="btnDetails" layoutX="287.0" layoutY="209.0" mnemonicParsing="false" onAction="#btnDetails_Pressed" text="i" />
            <Button fx:id="btnDeleteCourse" layoutX="250.0" layoutY="317.0" mnemonicParsing="false" onAction="#btnDeleteCourse_Pressed" text="Eliminar Curso" />
            <Button fx:id="btnEditCourse" layoutX="256.0" layoutY="274.0" mnemonicParsing="false" onAction="#btnEditCourse_Pressed" text="Editar Curso" />
         </children>
      </Pane>
      <Button fx:id="btnLogout" layoutX="531.0" layoutY="369.0" mnemonicParsing="false" onAction="#btnLogout_Pressed" style="-fx-background-radius: 0;" text="Logout" />
      <Button fx:id="btnBack" layoutX="24.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnBack_Pressed" style="-fx-background-radius: 0;" text="Back" />
      <Button fx:id="btnReload" layoutX="518.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnReload_Pressed" style="-fx-background-radius: 0;" text="R" />
      <Button fx:id="btnLanguage" layoutX="561.0" layoutY="28.0" mnemonicParsing="false" onAction="#btnLanguage_Pressed" style="-fx-background-radius: 0;" text="ES" />
   </children>
</AnchorPane>
