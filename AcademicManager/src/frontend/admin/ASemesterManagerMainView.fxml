<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.admin.ASemesterManagerMainViewController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="labelSemesterEditorWelcomeMessage" layoutX="24.0" layoutY="67.0" prefHeight="42.0" prefWidth="299.0" text="Este es el editor de Semestres, para comenzar selecciona un semestre a editar o elige crear uno nuevo" wrapText="true" />
            <Button fx:id="btnCreateSemester" layoutX="52.0" layoutY="299.0" mnemonicParsing="false" onAction="#btnCreateSemester_Pressed" style="-fx-background-radius: 0;" text="Crear Semestre" />
            <ChoiceBox fx:id="chBxSemesters" layoutX="52.0" layoutY="127.0" prefHeight="25.0" prefWidth="172.0" style="-fx-background-radius: 0;" />
            <Button fx:id="btnEditSemester" layoutX="53.0" layoutY="154.0" mnemonicParsing="false" onAction="#btnEditSemester_Pressed" style="-fx-background-radius: 0;" text="Editar Semestre" />
         </children>
      </Pane>
      <Button fx:id="btnLogout" layoutX="547.0" layoutY="383.0" mnemonicParsing="false" onAction="#btnLogout_Pressed" style="-fx-background-radius: 0;" text="Logout" />
      <Button fx:id="btnBack" layoutX="10.0" layoutY="11.0" mnemonicParsing="false" onAction="#btnBack_Pressed" style="-fx-background-radius: 0;" text="Back" />
      <Button fx:id="btnReload" layoutX="534.0" layoutY="11.0" mnemonicParsing="false" onAction="#btnReload_Pressed" style="-fx-background-radius: 0;" text="R" />
      <Button fx:id="btnLanguage" layoutX="574.0" layoutY="11.0" mnemonicParsing="false" onAction="#btnLanguage_Pressed" style="-fx-background-radius: 0;" text="ES" />
   </children>
</AnchorPane>
